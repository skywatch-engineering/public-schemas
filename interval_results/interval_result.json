{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/skywatch-engineering/public-schemas/master/interval_results/interval_result.json",
    "title": "JSON schema for Interval Result on EarthCacheâ„¢",
    "description": "<describe an interval result>",
    "type": "object",
    "required": [
        "id",
        "output_id",
        "pipeline_id",
        "status",
        "interval",
        "created_at",
        "updated_at"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "string",
            "minLength": 1,
            "examples": [
                "62d161f0-00ea-41b9-8e12-cecb95d21bba"
            ]
        },
        "output_id": {
            "type": "string",
            "minLength": 1,
            "examples": [
                "a8fc3dde-a3e8-11e7-9793-ae4260ee3b4b"
            ]
        },
        "pipeline_id": {
            "type": "string",
            "minLength": 1
        },
        "status": {
            "enum": [
                "complete",
                "processing"
            ]
        },
        "message": {
            "type": "string",
            "examples": [
                "Processing complete. 1 result available"
            ]
        },
        "interval": {
            "type": "object",
            "required": [
                "start_date",
                "end_date"
            ],
            "properties": {
                "start_date": {
                    "format": "date",
                    "minLength": 1
                },
                "end_date": {
                    "format": "date",
                    "minLength": 1
                }
            }
        },
        "results": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "status"
                ],
                "properties": {
                    "preview_url": {
                        "type": "string",
                        "minLength": 1
                    },
                    "visual_url": {
                        "type": "string",
                        "minLength": 1
                    },
                    "analytics": {
                        "type": "string",
                        "minLength": 1
                    },
                    "capture_time": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "metadata": {
                        "type": "object"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                }
            }
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time"
        }
    }
}